<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>classesEntity</managed-bean-name>
        <managed-bean-class>sessionBeans.ClassesSessionBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>editClassesEntity</from-outcome>
            <to-view-id>/cenas/EditClassesEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewClassesEntity</from-outcome>
            <to-view-id>/cenas/ViewClassesEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>classesEntityList</from-outcome>
            <to-view-id>/cenas/ListClassesEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createClassesEntity</from-outcome>
            <to-view-id>/cenas/CreateClassesEntity.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>